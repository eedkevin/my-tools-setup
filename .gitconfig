[alias]
  co = checkout
  cob = checkout -b
  cm = commit
  mm = commit -m
  m = commit -m
  st = status
  br = branch
  b = branch
  ba = branch -a
  bd = branch -d
  bdd = branch -D
  pll = pull
  l = pull
  psh = push
  pp = push
  p = push
  sth = stash
  sh = stash
  stp = stash pop
  sp = stash pop
  cpk = cherry-pick
  pk = cherry-pick
  df = diff
  lg = log
  aa = add -A
  a = add -A

[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true

[pull]
	ff = only
